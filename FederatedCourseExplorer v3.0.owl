#################################################################

###  http://www.federatedcourseexplorer.org#attendedBy
:attendedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :attends ;
            rdfs:domain :Course ;
            rdfs:range :StudyProgram .


###  http://www.federatedcourseexplorer.org#attends
:attends rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :StudyProgram ;
         rdfs:range :Course .


###  http://www.federatedcourseexplorer.org#employedBy
:employedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :employs ;
            rdfs:domain :Lecturer ;
            rdfs:range :University .


###  http://www.federatedcourseexplorer.org#employs
:employs rdf:type owl:ObjectProperty ;
         rdfs:domain :University ;
         rdfs:range :Lecturer .


###  http://www.federatedcourseexplorer.org#offeredBy
:offeredBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :offers ;
           rdfs:domain :StudyProgram ;
           rdfs:range :University .


###  http://www.federatedcourseexplorer.org#offers
:offers rdf:type owl:ObjectProperty ;
        rdfs:domain :University ;
        rdfs:range :StudyProgram .


###  http://www.federatedcourseexplorer.org#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :University ;
        rdfs:range <http://www.federatedcourseexplorer.org#4TU> .


###  http://www.federatedcourseexplorer.org#teachedBy
:teachedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :teaches .


###  http://www.federatedcourseexplorer.org#teaches
:teaches rdf:type owl:ObjectProperty ;
         rdfs:domain :Lecturer ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :teaches ;
                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onClass :Course
                     ] ;
         rdfs:range :Course .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain [ rdf:type owl:Restriction ;
                                    owl:onProperty owl:topObjectProperty ;
                                    owl:someValuesFrom <http://www.federatedcourseexplorer.org#4TU>
                                  ] ;
                      rdfs:range [ rdf:type owl:Restriction ;
                                   owl:onProperty owl:topObjectProperty ;
                                   owl:someValuesFrom :University
                                 ] .


